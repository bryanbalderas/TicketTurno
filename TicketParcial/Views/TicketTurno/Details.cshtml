@model TicketParcial.Models.TicketTurnoModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<a type="button" asp-action="Index" class="btn btn-secondary mb-5">
    <i class="bi bi-arrow-return-left mr-1"></i>
    Regresar
</a>

<div class="row d-flex justify-content-around">
    <div class="col-12">
        <div class="card shadow-sm">
            <div class="card-content table-responsive">
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.ID)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.nombreRealiza)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.curp)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.nombre)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.paterno)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.materno)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.telefono)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.celular)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.correo)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.municipio)
                            </th>
                            <th class="text-center align-middle">
                                @Html.DisplayNameFor(model => model.asunto)
                            </th>
                            <th class="text-center align-middle">
                                Nivel
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.ID)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.nombreRealiza)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.curp)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.nombre)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.paterno)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.materno)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.telefono)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.celular)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.correo)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.municipio.descripcion)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.asunto.descripcion)
                            </td>
                            <td class="text-center align-middle">
                                @Html.DisplayFor(model => model.nivel.descripcion)
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>